{
  "name": "dasboard",
  "version": "1.0.0",
  "type": "module",
  "description": "A comprehensive dashboard application for automotive dealerships",
  "author": "The DAS Board Team",
  "license": "ISC",
  "keywords": ["dashboard", "dealership", "automotive", "finance", "react", "typescript"],
  "engines": {
    "node": ">=18.18.0 <19.0.0 || >=20.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite --port 5173 --strictPort",
    "dev:alt": "vite --port 5174 --strictPort",
    "dev:alt2": "vite --port 5175 --strictPort",
    "dev:with-api": "concurrently \"npm run dev\" \"npm run api:dev\"",
    "api:dev": "cd server && npm install && npm run dev",
    "api:start": "cd server && npm start",
    "build": "vite build",
    "build:typecheck": "tsc --noEmit && vite build",
    "build:analyze": "vite build --analyze",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:auth": "vitest run --grep=\"AuthContext|AuthErrorBoundary\"",
    "test:build": "tsx src/buildTest.ts",
    "test:legacy": "tsx src/test.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "stop": "npx kill-port 5173 5174 5175 3001",
    "deploy:security": "node scripts/deploy-security.js",
    "deploy:manual": "echo 'ðŸ“‹ Manual deployment required. See DEPLOYMENT-GUIDE.md for instructions.' && echo 'ðŸŽ¯ Dashboard: https://app.supabase.com/project/iugjtokydvbcvmrpeziv'",
    "security:audit": "npm audit --audit-level moderate",
    "security:audit:fix": "npm audit fix",
    "security:audit:force": "npm audit fix --force",
    "security:outdated": "npm outdated",
    "security:validate": "node scripts/validate-security.js",
    "security:full-check": "npm run security:audit && npm run security:outdated && npm run typecheck && npm run lint",
    "deps:check": "npm ls --depth=0",
    "deps:update": "npm update",
    "deps:update-interactive": "npx npm-check -u",
    "clean": "rm -rf node_modules dist .vite && npm install",
    "prepare": "npm run security:audit"
  },
